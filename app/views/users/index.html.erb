<h1>Users</h1>

<table>
  <tr>
    <th>Name</th>
    <th>ID</th>
    <th>Profile</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.id %></td>
      <td><%= link_to "Go to", user_path(user) %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "Create new User", new_user_path %></p>
